<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.thegiven.tester">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.Pickandchoose"
            android:label="@string/title_activity_pickandchoose" />
        <activity
            android:name=".Activities.Globalquestions"
            android:label="@string/title_activity_globalquestions" />
        <activity
            android:name=".Activities.Playrooms"
            android:label="@string/title_activity_playrooms"></activity>
    </application>

</manifest>
